import wollok.game.*
import jugador.*
import mundo.*
import config.*

describe "tests de la isla enemigos" {
	method initialize() {
		stats.cambiarPersonaje(new Personaje(image="luffyQuieto.jpg", position = game.at(0, game.height() / 2), positionAnterior = null))
	}
	
	test "restarVida() saca vida a jugador" {
		stats.restarVida()
		assert.that(stats.vida().size() === 4)
	}
}